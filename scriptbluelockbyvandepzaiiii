for _, nut in pairs(DanhSachNut) do
    TaoNut(nut[1], nut[2], nut[3])
end

-- Thông báo
local function ThongBao(tieuDe, noiDung)
    warn("[Vandepzaiii Hub V4.1] " .. tieuDe .. ": " .. noiDung)
end
ThongBao("Khởi Động", "Hack đã sẵn sàng! Nhấn F để đóng/mở menu.")

-- Aimbot
local function TimNguoiGanNhat()
    local ganNhat, khoangCach = nil, math.huge
    for _, nguoi in pairs(Services.Players:GetPlayers()) do
        if nguoi ~= Player and nguoi.Character and nguoi.Character:FindFirstChild("HumanoidRootPart") then
            local kc = (RootPart.Position - nguoi.Character.HumanoidRootPart.Position).Magnitude
            if kc < khoangCach and kc <= getgenv().Vandepzaiii.Aimbot.TamNhin then
                khoangCach = kc
                ganNhat = nguoi
            end
        end
    end
    return ganNhat
end

Services.RunService.RenderStepped:Connect(function()
    if getgenv().Vandepzaiii.Aimbot.Bat then
        local mucTieu = TimNguoiGanNhat()
        if mucTieu then
            local viTriMucTieu = mucTieu.Character.HumanoidRootPart.Position
            local viTriCam = Camera.CFrame.Position
            local cfMoi = CFrame.new(viTriCam, viTriMucTieu)
            Camera.CFrame = Camera.CFrame:Lerp(cfMoi, getgenv().Vandepzaiii.Aimbot.DoMuot)
        end
    end
end)

-- Ball Aura (Tăng phạm vi tối đa)
coroutine.wrap(function()
    while true do
        if getgenv().Vandepzaiii.BallAura.Bat then
            local bong = Services.Workspace:FindFirstChild("Football")
            if bong and bong:IsA("BasePart") then
                local kc = (RootPart.Position - bong.Position).Magnitude
                if kc <= getgenv().Vandepzaiii.BallAura.TamHut then
                    bong.CFrame = RootPart.CFrame * CFrame.new(0, 0, -2) -- Đưa bóng sát chân
                    pcall(function()
                        Services.ReplicatedStorage.Packages.Knit.Services.BallService.RE.Dribble:FireServer()
                        ThongBao("Ball Aura", "Đã hút bóng, kc: " .. kc)
                    end)
                else
                    -- Di chuyển nhân vật đến bóng nếu quá xa
                    RootPart.CFrame = bong.CFrame + Vector3.new(0, 3, 0)
                    wait(0.1)
                    bong.CFrame = RootPart.CFrame * CFrame.new(0, 0, -2)
                    pcall(function()
                        Services.ReplicatedStorage.Packages.Knit.Services.BallService.RE.Dribble:FireServer()
                        ThongBao("Ball Aura", "Đã di chuyển đến bóng, kc: " .. kc)
                    end)
                end
            end
        end
        wait(0.01)
    end
end)()

-- Auto Skill
coroutine.wrap(function()
    while true do
        if getgenv().Vandepzaiii.AutoSkill then
            local bong = Services.Workspace:FindFirstChild("Football")
            if bong then
                Services.ReplicatedStorage.Packages.Knit.Services.BallService.RE.Dribble:FireServer()
                wait(0.03)
                local mucTieu = Services.Workspace.Goals[Player.Team.Name == "Home" and "Away" or "Home"]
                if mucTieu then
                    Services.ReplicatedStorage.Packages.Knit.Services.BallService.RE.Shoot:FireServer(1500, (mucTieu.Position - bong.Position).Unit, bong, mucTieu.Position)
                end
                wait(0.03)
                Services.ReplicatedStorage.Packages.Knit.Services.BallService.RE.Slide:FireServer()
            end
        end
        wait(0.1)
    end
end)()

-- Tốc Độ & Nhảy Cao
Services.RunService.Stepped:Connect(function()
    if Character and Character:FindFirstChild("Humanoid") then
        Humanoid.WalkSpeed = getgenv().Vandepzaiii.TocDo
        Humanoid.JumpPower = getgenv().Vandepzaiii.NhayCao
    end
end)

-- Stamina Vô Hạn
coroutine.wrap(function()
    while true do
        if getgenv().Vandepzaiii.StaminaVoHan then
            local stats = Player:FindFirstChild("PlayerStats")
            if stats and stats:FindFirstChild("Stamina") then
                stats.Stamina.Value = math.huge
            end
        end
        wait(0.02)
    end
end)()

-- ESP
local ESPInstances = {}
local function ThemESP(nguoi)
    if nguoi ~= Player and nguoi.Character then
        if not ESPInstances[nguoi.Name] then
            local Billboard = Instance.new("BillboardGui", nguoi.Character:WaitForChild("Head"))
            Billboard.Size = UDim2.new(0, 100, 0, 50)
            Billboard.StudsOffset = Vector3.new(0, 3, 0)
            local Ten = Instance.new("TextLabel", Billboard)
            Ten.Size = UDim2.new(1, 0, 1, 0)
            Ten.Text = nguoi.Name .. "\n" .. (nguoi.Team and nguoi.Team.Name or "Không Team")
            Ten.TextColor3 = Color3.fromRGB(255, 0, 0)
            Ten.BackgroundTransparency = 1
            Ten.TextScaled = true
            local Vien = Instance.new("UIStroke", Ten)
            Vien.Color = Color3.fromRGB(255, 255, 0)
            Vien.Thickness = 1
            ESPInstances[nguoi.Name] = Billboard
        end
    end
end

Services.RunService.RenderStepped:Connect(function()
    if getgenv().Vandepzaiii.ESP.Bat then
        for _, nguoi in pairs(Services.Players:GetPlayers()) do
            if nguoi.Character then ThemESP(nguoi) end
        end
    else
        for _, esp in pairs(ESPInstances) do
            esp:Destroy()
        end
        ESPInstances = {}
    end
end)

-- Auto Join Team
coroutine.wrap(function()
    while true do
        if getgenv().Vandepzaiii.AutoJoinTeam then
            local lobby = Services.ReplicatedStorage:FindFirstChild("LobbyService")
            if lobby and lobby:FindFirstChild("RE") then
                pcall(function()
                    lobby.RE.JoinTeam:FireServer("Home")
                    wait(0.5)
                    if not Player.Team or Player.Team.Name ~= "Home" then
                        lobby.RE.JoinTeam:FireServer("Away")
                    end
                    wait(0.5)
                    local gameService = Services.ReplicatedStorage:FindFirstChild("GameService")
                    if gameService and gameService:FindFirstChild("RE") then
                        gameService.RE.StartGame:FireServer()
                    end
                    ThongBao("Auto Join", "Đã thử join team: " .. (Player.Team and Player.Team.Name or "Chưa join"))
                end)
            end
        else
            wait(1)
        end
        wait(2)
    end
end)()

-- Auto Farm
coroutine.wrap(function()
    while true do
        if getgenv().Vandepzaiii.AutoFarm then
            local bong = Services.Workspace:FindFirstChild("Football")
            if bong and Player.Team then
                local kc = (RootPart.Position - bong.Position).Magnitude
                if kc > 5 then
                    RootPart.CFrame = bong.CFrame + Vector3.new(0, 3, 0)
                    wait(0.1)
                end
                Services.ReplicatedStorage.Packages.Knit.Services.BallService.RE.Dribble:FireServer()
                wait(0.05)
                local mucTieu = Services.Workspace.Goals:FindFirstChild(Player.Team.Name == "Home" and "Away" or "Home")
                if mucTieu then
                    local huong = (mucTieu.Position - bong.Position).Unit
                    bong.Velocity = huong * 500
                    local shootData = {
                        Power = 3500,
                        Direction = huong,
                        BallId = bong,
                        TargetPosition = mucTieu.Position + Vector3.new(0, 2, 0)
                    }
                    pcall(function()
                        Services.ReplicatedStorage.Packages.Knit.Services.BallService.RE.Shoot:FireServer(shootData.Power, shootData.Direction, shootData.BallId, shootData.TargetPosition)
                        ThongBao("Auto Farm", "Đã sút với Power: " .. shootData.Power .. ", Target: " .. tostring(mucTieu.Position))
                        Humanoid:LoadAnimation(game.ReplicatedStorage.Animations.ShootAnimation):Play()
                    end)
                end
            end
        end
        wait(0.03)
    end
end)()

-- Auto GK
coroutine.wrap(function()
    while true do
        if getgenv().Vandepzaiii.AutoGK and Player.Team then
            local khungThanh = Services.Workspace.Goals[Player.Team.Name]
            if khungThanh then
                local viTriGK = khungThanh.Position + Vector3.new(0, 0, -10)
                RootPart.CFrame = CFrame.new(viTriGK)
                local bong = Services.Workspace:FindFirstChild("Football")
                if bong then
                    local kc = (viTriGK - bong.Position).Magnitude
                    if kc < 20 then
                        local huong = (bong.Position - viTriGK).Unit
                        RootPart.CFrame = CFrame.new(viTriGK, bong.Position)
                        if kc < 10 then
                            bong.Velocity = -huong * 150
                            Services.ReplicatedStorage.Packages.Knit.Services.BallService.RE.Dribble:FireServer()
                            pcall(function()
                                Humanoid:LoadAnimation(game.ReplicatedStorage.Animations.SaveAnimation):Play()
                            end)
                        end
                    end
                end
            end
        end
        wait(0.02)
    end
end)()

-- Kaiser Impact
local function KaiserShoot(bong, mucTieu)
    local batDau = bong.Position
    local ketThuc = mucTieu.Position + Vector3.new(0, 2, 0)
    local thoiGian = 0.5
    local chieuCao = 30
    local cong = 8
    local thoiGianBatDau = tick()
    Services.RunService.RenderStepped:Connect(function()
        local t = (tick() - thoiGianBatDau) / thoiGian
        if t > 1 then
            bong.Velocity = Vector3.new(0, 0, 0)
            return
        end
        local viTri = batDau:Lerp(ketThuc, t)
        local doCong = math.sin(t * math.pi) * chieuCao
        local offset = math.sin(t * math.pi * cong) * 12
        bong.Velocity = (ketThuc - batDau).Unit * 150 + Vector3.new(0, doCong, 0)
        bong.CFrame = CFrame.new(viTri.X + offset, batDau.Y + doCong, viTri.Z)
        Services.ReplicatedStorage.Packages.Knit.Services.BallService.RE.Shoot:FireServer(1500, nil, nil, ketThuc)
    end)
end

-- Curve Shot
local function CurveShoot(bong, mucTieu)
    local batDau = bong.Position
    local ketThuc = mucTieu.Position + Vector3.new(0, 2, 0)
    local thoiGian = 0.7
    local chieuCao = 25
    local cong = 10
    local thoiGianBatDau = tick()
    Services.RunService.RenderStepped:Connect(function()
        local t = (tick() - thoiGianBatDau) / thoiGian
        if t > 1 then
            bong.Velocity = Vector3.new(0, 0, 0)
            return
        end
        local viTri = batDau:Lerp(ketThuc, t)
        local doCong = math.sin(t * math.pi) * chieuCao
        local offset = math.cos(t * math.pi * cong) * 20
        bong.Velocity = (ketThuc - batDau).Unit * 120 + Vector3.new(0, doCong, 0)
        bong.CFrame = CFrame.new(viTri.X + offset, batDau.Y + doCong, viTri.Z) * CFrame.Angles(0, t * math.rad(720), 0)
        Services.ReplicatedStorage.Packages.Knit.Services.BallService.RE.Shoot:FireServer(1200, nil, nil, ketThuc)
    end)
end

Services.ReplicatedStorage.Packages.Knit.Services.BallService.RE.Shoot.OnClientEvent:Connect(function()
    local bong = Services.Workspace:FindFirstChild("Football")
    if bong then
        local mucTieu = Services.Workspace.Goals[Player.Team.Name == "Home" and "Away" or "Home"]
        if getgenv().Vandepzaiii.KaiserImpact then
            KaiserShoot(bong, mucTieu)
        elseif getgenv().Vandepzaiii.CurveShot then
            CurveShoot(bong, mucTieu)
        end
    end
end)

-- Auto Goal
coroutine.wrap(function()
    while true do
        if getgenv().Vandepzaiii.AutoGoal then
            local bong = Services.Workspace:FindFirstChild("Football")
            if bong and Player.Team then
                local kc = (RootPart.Position - bong.Position).Magnitude
                if kc > 5 then
                    RootPart.CFrame = bong.CFrame + Vector3.new(0, 3, 0)
                    wait(0.1)
                end
                local mucTieu = Services.Workspace.Goals[Player.Team.Name == "Home" and "Away" or "Home"]
                if mucTieu then
                    local huong = (mucTieu.Position - bong.Position).Unit
                    bong.Velocity = huong * 600
                    local shootData = {
                        Power = 4000,
                        Direction = huong,
                        BallId = bong,
                        TargetPosition = mucTieu.Position + Vector3.new(0, 5, 0)
                    }
                    pcall(function()
                        Services.ReplicatedStorage.Packages.Knit.Services.BallService.RE.Shoot:FireServer(shootData.Power, shootData.Direction, shootData.BallId, shootData.TargetPosition)
                        ThongBao("Auto Goal", "Đã sút với Power: " .. shootData.Power .. ", Target: " .. tostring(shootData.TargetPosition))
                        Humanoid:LoadAnimation(game.ReplicatedStorage.Animations.ShootAnimation):Play()
                    end)
                end
            end
        end
        wait(0.02)
    end
end)()

-- Server Crasher
coroutine.wrap(function()
    while true do
        if getgenv().Vandepzaiii.ServerCrasher then
            for i = 1, 200 do
                Services.ReplicatedStorage.Packages.Knit.Services.BallService.RE.Shoot:FireServer(math.huge, nil, nil, Vector3.new(math.random(-10000, 10000), math.random(-10000, 10000), math.random(-10000, 10000)))
            end
        end
        wait(0.03)
    end
end)()

-- Hitbox
coroutine.wrap(function()
    while true do
        if getgenv().Vandepzaiii.Hitbox.Bat then
            for _, bong in pairs(Services.Workspace:GetChildren()) do
                if bong.Name == "Football" and bong:FindFirstChild("Hitbox") then
                    bong.Hitbox.Size = Vector3.new(getgenv().Vandepzaiii.Hitbox.KichThuoc, getgenv().Vandepzaiii.Hitbox.KichThuoc, getgenv().Vandepzaiii.Hitbox.KichThuoc)
                    bong.Hitbox.Transparency = 0.7
                    bong.Hitbox.Material = Enum.Material.Neon
                end
            end
        end
        wait(0.05)
    end
end)()

-- God Mode
coroutine.wrap(function()
    while true do
        if getgenv().Vandepzaiii.GodMode then
            pcall(function()
                Humanoid:ChangeState(Enum.HumanoidStateType.Physics)
                RootPart.Anchored = true
                wait(0.005)
                RootPart.Anchored = false
                Humanoid.Health = math.huge
            end)
        end
        wait(0.02)
    end
end)()

-- Anti Ban
coroutine.wrap(function()
    if getgenv().Vandepzaiii.AntiBan then
        hookfunction(game:GetService("Players").LocalPlayer.Kick, function() end)
        for _, v in pairs(getgc(true)) do
            if type(v) == "function" and getfenv(v).script == game:GetService("ReplicatedStorage") then
                hookfunction(v, function() end)
            end
        end
        game:GetService("NetworkClient").Connection:Connect(function()
            return false
        end)
    end
end)()

-- Fake Lag
coroutine.wrap(function()
    while true do
        if getgenv().Vandepzaiii.FakeLag then
            Services.RunService:Set3dRenderingEnabled(false)
            wait(0.05)
            Services.RunService:Set3dRenderingEnabled(true)
        end
        wait(0.1)
    end
end)()

-- Chống AFK
Services.Players.LocalPlayer.Idled:Connect(function()
    Services.VirtualInputManager:SendKeyEvent(true, Enum.KeyCode.Space, false, nil)
    wait(0.05)
    Services.VirtualInputManager:SendKeyEvent(false, Enum.KeyCode.Space, false, nil)
end)

-- Thông báo
ThongBao("Hoàn Tất", "Hack đã sẵn sàng! Nhấn F để đóng/mở menu.")
